<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>

        <!-- FABのデフォルトスタイル -->
        <item name="floatingActionButtonStyle">@style/FloatingActionButtonStyle</item>

        <!-- ボタンのデフォルトスタイル -->
        <item name="materialButtonStyle">@style/ButtonStyle</item>

        <!-- ダイアログのデフォルトスタイル -->
        <item name="android:alertDialogTheme">@style/AlertDialogStyle</item>

        <!-- enable fragment transition -->
        <item name="android:windowContentTransitions">true</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>

        <!-- action bar area -->
        <item name="android:actionOverflowButtonStyle">@style/ActionBarButtonStyle</item>
        <!-- バーの背景色 -->
        <item name="android:statusBarColor">@color/colorPrimary</item>
        <!-- titleの文字色 -->
        <item name="android:textColorPrimary">@color/colorPrimaryText</item>
        <!-- subtitleの文字色 -->
        <item name="android:textColorSecondary">@color/colorPrimaryText</item>
        <!-- ポップアップ部分の文字色 -->
        <item name="android:textColor">@color/textColor</item>

        <!-- ツールバーの色を他の部分に渡すために使用 -->
        <item name="toolbarBackground">@color/colorPrimary</item>

        <!-- SearchView -->
        <item name="searchViewStyle">@style/SearchViewStyle.Toolbar</item>
        <item name="autoCompleteTextViewStyle">@style/AutoCompleteTextView.Toolbar</item>

        <!-- TabLayout -->
        <item name="tabSelectedTextColor">@color/colorPrimary</item>
        <item name="tabTextColor">@color/disabledColor</item>
        <item name="tabIndicatorColor">@color/colorPrimary</item>

        <!-- for NumberPicker -->
        <item name="colorControlNormal">@color/colorPrimary</item>

        <!-- SnackBar -->
        <item name="snackBarBackgroundColor">@color/snackBarBackgroundColor</item>
        <item name="snackBarTextColor">@color/snackBarTextColor</item>
        <item name="snackBarActionTextColor">@color/snackBarActionTextColor</item>
    </style>

    <!-- Light Theme -->
    <style name="AppTheme.Light">
        <item name="panelBackground">@color/panelBackground</item>
        <item name="tabBackground">@color/tabBackground</item>
        <item name="textColor">@color/textColor</item>
        <item name="textColorHint">@color/textColorHint</item>
        <item name="dividerColor">@color/dividerColor</item>
        <item name="swipeRefreshBackground">@color/swipeRefreshBackground</item>
        <item name="bookmarkDialogBackground">@color/bookmarkDialogBackground</item>
        <item name="entryCommentBackground">@color/entryCommentBackground</item>
        <item name="bookmarkDetailTopBackground">@color/BookmarkDetailTopBackground</item>
        <item name="tagTextColor">@color/tagColor</item>
        <item name="bookmarkCountColor">@color/bookmarkUsersCountText</item>
        <!-- オプションメニュー背景色 -->
        <item name="colorSurface">@color/panelBackground</item>

        <item name="android:spinnerStyle">@style/SpinnerStyle.Light</item>
        <item name="alertDialogTheme">@style/AlertDialogStyle.Light</item>
        <item name="android:textViewStyle">@style/TextViewStyle.Light</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Light</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Light</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetDialogTheme.Light</item>

        <!-- ブラウザ画面ボトムシート「戻る/進む」履歴の現在表示中ページ項目の背景色 -->
        <item name="browserBackStackCurrentPageBackground">@color/browserBackStackCurrentPageBackground</item>
    </style>

    <!-- Dark Theme -->
    <style name="AppTheme.Dark">
        <item name="panelBackground">@color/panelBackgroundDark</item>
        <item name="tabBackground">@color/tabBackgroundDark</item>
        <item name="textColor">@color/textColorDark</item>
        <item name="textColorHint">@color/textColorHintDark</item>
        <item name="dividerColor">@color/dividerColorDark</item>
        <item name="swipeRefreshBackground">@color/swipeRefreshBackgroundDark</item>
        <item name="bookmarkDialogBackground">@color/bookmarkDialogBackgroundDark</item>
        <item name="entryCommentBackground">@color/entryCommentBackgroundDark</item>
        <item name="bookmarkDetailTopBackground">@color/BookmarkDetailTopBackgroundDark</item>
        <item name="tagTextColor">@color/tagColorDark</item>
        <item name="bookmarkCountColor">@color/bookmarkUsersCountTextDark</item>
        <!-- オプションメニュー背景色 -->
        <item name="colorSurface">@color/panelBackgroundDark</item>

        <item name="android:spinnerStyle">@style/SpinnerStyle.Dark</item>
        <item name="alertDialogTheme">@style/AlertDialogStyle.Dark</item>
        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Dark</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetDialogTheme.Dark</item>

        <!-- ブラウザ画面ボトムシート「戻る/進む」履歴の現在表示中ページ項目の背景色 -->
        <item name="browserBackStackCurrentPageBackground">@color/browserBackStackCurrentPageBackgroundDark</item>
    </style>

    <!-- Extreme Dark Theme -->
    <style name="AppTheme.ExDark">
        <!-- ツールバーの色を他の部分に渡すために使用 -->
        <item name="toolbarBackground">@color/panelBackgroundExDark</item>

        <item name="android:toolbarStyle">@style/ToolbarStyleExDark</item>
        <item name="toolbarStyle">@style/ToolbarStyleExDark</item>
        <item name="android:statusBarColor">@color/panelBackgroundExDark</item>
        <item name="panelBackground">@color/panelBackgroundExDark</item>
        <item name="tabBackground">@color/tabBackgroundExDark</item>
        <item name="textColor">@color/textColorExDark</item>
        <item name="textColorHint">@color/textColorHintDark</item>
        <item name="dividerColor">@color/dividerColorDark</item>
        <item name="swipeRefreshBackground">@color/swipeRefreshBackgroundExDark</item>
        <item name="bookmarkDialogBackground">@color/bookmarkDialogBackgroundExDark</item>
        <item name="entryCommentBackground">@color/entryCommentBackgroundExDark</item>
        <item name="bookmarkDetailTopBackground">@color/panelBackgroundExDark</item>
        <item name="tagTextColor">@color/tagColorDark</item>
        <item name="bookmarkCountColor">@color/bookmarkUsersCountTextDark</item>
        <!-- オプションメニュー背景色 -->
        <item name="colorSurface">@color/panelBackgroundDark</item>

        <item name="android:spinnerStyle">@style/SpinnerStyle.Dark</item>
        <item name="alertDialogTheme">@style/AlertDialogStyle.Dark</item>
        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Dark</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetDialogTheme.ExDark</item>

        <!-- ブラウザ画面ボトムシート「戻る/進む」履歴の現在表示中ページ項目の背景色 -->
        <item name="browserBackStackCurrentPageBackground">@color/browserBackStackCurrentPageBackgroundDark</item>
    </style>

    <style name="ToolbarStyleExDark" parent="Widget.AppCompat.Toolbar">
        <item name="android:background">@color/panelBackgroundExDark</item>
    </style>

    <!-- スプラッシュ画面のテーマ -->
    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="textColor">@color/colorPrimaryText</item>
        <item name="android:textColor">@color/colorPrimaryText</item>

        <item name="android:windowEnterTransition">@android:transition/fade</item>
        <item name="android:windowExitTransition">@android:transition/fade</item>
    </style>

    <style name="ActionBarButtonStyle" parent="@style/Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/colorPrimaryText</item>
    </style>

    <!-- 設定画面のトグルボタン -->
    <style name="PreferencesToggleButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:backgroundTint">@drawable/preferences_toggle_background_selector</item>
        <item name="android:textColor">@drawable/preferences_toggle_text_color_selector</item>
    </style>

    <!-- ブクマ投稿ダイアログ用 -->
    <style name="AppDialogTheme" parent="Theme.AppCompat.Dialog">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>

        <!-- ダイアログのデフォルトスタイル -->
        <item name="android:alertDialogTheme">@style/AlertDialogStyle</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <!-- enable fragment transition -->
        <item name="android:windowContentTransitions">true</item>

        <!-- 画面幅占有比率 -->
        <item name="android:windowMinWidthMajor">@dimen/post_bookmark_dialog_width_ratio</item>
        <item name="android:windowMinWidthMinor">@dimen/post_bookmark_dialog_width_ratio</item>
    </style>

    <!-- Light Theme -->
    <style name="AppDialogTheme.Light">
        <item name="panelBackground">@color/panelBackground</item>
        <item name="tabBackground">@color/tabBackground</item>
        <item name="textColor">@color/textColor</item>
        <item name="textColorHint">@color/textColorHint</item>
        <item name="dividerColor">@color/dividerColor</item>
        <item name="swipeRefreshBackground">@color/swipeRefreshBackground</item>
        <item name="bookmarkDialogBackground">@color/bookmarkDialogBackground</item>
        <item name="entryCommentBackground">@color/entryCommentBackground</item>
        <item name="tagTextColor">@color/tagColor</item>

        <item name="android:textViewStyle">@style/TextViewStyle.Light</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Light</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Light</item>
    </style>

    <!-- Dark Theme -->
    <style name="AppDialogTheme.Dark">
        <item name="panelBackground">@color/panelBackgroundDark</item>
        <item name="tabBackground">@color/tabBackgroundDark</item>
        <item name="textColor">@color/textColorDark</item>
        <item name="textColorHint">@color/textColorHintDark</item>
        <item name="dividerColor">@color/dividerColorDark</item>
        <item name="swipeRefreshBackground">@color/swipeRefreshBackgroundDark</item>
        <item name="bookmarkDialogBackground">@color/bookmarkDialogBackgroundDark</item>
        <item name="entryCommentBackground">@color/entryCommentBackgroundDark</item>
        <item name="tagTextColor">@color/tagColorDark</item>

        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Dark</item>
    </style>

    <!-- Extreme Dark Theme -->
    <style name="AppDialogTheme.ExDark">
        <item name="panelBackground">@color/panelBackgroundExDark</item>
        <item name="tabBackground">@color/tabBackgroundExDark</item>
        <item name="textColor">@color/textColorExDark</item>
        <item name="textColorHint">@color/textColorHintDark</item>
        <item name="dividerColor">@color/dividerColorDark</item>
        <item name="swipeRefreshBackground">@color/swipeRefreshBackgroundExDark</item>
        <item name="bookmarkDialogBackground">@color/bookmarkDialogBackgroundExDark</item>
        <item name="entryCommentBackground">@color/entryCommentBackgroundExDark</item>
        <item name="tagTextColor">@color/tagColorDark</item>

        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Dark</item>
    </style>

    <!-- ログイン画面のEditText -->
    <style name="LoginEditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="colorControlNormal">@color/colorControlNormal</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="android:textCursorDrawable">@color/textCursorDrawable</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textColorHint">@color/textColorHint</item>
        <item name="android:textColorHighlight">@color/colorPrimary</item>
    </style>

    <style name="MastodonLoginEditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="colorControlNormal">@color/colorControlNormal</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="android:textCursorDrawable">@color/textCursorDrawable</item>
        <item name="android:textColor">@color/mastodonTextColor</item>
        <item name="android:textColorHint">@color/textColorHint</item>
        <item name="android:textColorHighlight">@color/colorPrimary</item>
    </style>

    <!-- AlertDialog -->
    <style name="AlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:windowTitleStyle">@style/AlertDialogTitleStyle</item>
        <item name="buttonBarPositiveButtonStyle">@style/ButtonBarButtonStyle</item>
        <item name="buttonBarNegativeButtonStyle">@style/ButtonBarButtonStyle</item>
        <item name="buttonBarNeutralButtonStyle">@style/ButtonBarButtonStyle</item>

        <item name="android:windowMinWidthMajor">@dimen/alert_dialog_width_ratio</item>
        <item name="android:windowMinWidthMinor">@dimen/alert_dialog_width_ratio</item>

        <!-- TabLayout -->
        <item name="tabSelectedTextColor">@color/colorPrimary</item>
        <item name="tabTextColor">@color/disabledColor</item>
        <item name="tabIndicatorColor">@color/colorPrimary</item>

        <!-- for NumberPicker -->
        <item name="colorControlNormal">@color/colorPrimary</item>
    </style>

    <!-- Light -->
    <style name="AlertDialogStyle.Light">
        <item name="colorBackgroundFloating">@color/dialogBackground</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="textColor">@color/textColor</item>
        <item name="android:textColorPrimary">@color/textColor</item>
        <item name="android:textColorSecondary">@color/textColor</item>
        <item name="textColorAlertDialogListItem">@color/textColor</item>
        <item name="tabBackground">@color/tabBackground</item>

        <item name="android:textViewStyle">@style/TextViewStyle.Light</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Light</item>
        <item name="android:textColorHint">@color/textColorHint</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Light</item>
    </style>

    <!-- Dark -->
    <style name="AlertDialogStyle.Dark">
        <item name="colorBackgroundFloating">@color/dialogBackgroundDark</item>
        <item name="android:textColor">@color/textColorDark</item>
        <item name="textColor">@color/textColorDark</item>
        <item name="android:textColorPrimary">@color/textColorDark</item>
        <item name="android:textColorSecondary">@color/textColorDark</item>
        <item name="textColorAlertDialogListItem">@color/textColorDark</item>
        <item name="tabBackground">@color/tabBackgroundDark</item>

        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="android:editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="android:textColorHint">@color/textColorHintDark</item>
        <item name="android:checkboxStyle">@style/CheckBoxStyle.Dark</item>
    </style>

    <style name="AlertDialogTitleStyle" parent="TextAppearance.AppCompat.Title">
        <item name="android:textColor">?textColor</item>
    </style>

    <style name="ButtonBarButtonStyle" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <!-- TabLayout -->
    <style name="TabLayoutStyle" parent="Widget.Design.TabLayout">
        <item name="tabTextAppearance">@android:style/TextAppearance.Widget.TabWidget</item>
        <item name="tabSelectedTextColor">?attr/tabSelectedTextColor</item>
        <item name="tabIndicatorColor">?attr/tabIndicatorColor</item>
        <item name="tabTextColor">?attr/tabTextColor</item>
        <item name="tabGravity">fill</item>
        <item name="tabMaxWidth">0dp</item>
    </style>

    <!-- Spinner -->
    <style name="SpinnerStyle" parent="Widget.AppCompat.Spinner">
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:dropDownSelector">@color/colorPrimaryText</item>
        <item name="subMenuArrow">@null</item>
        <item name="backgroundTint">@color/colorPrimaryText</item>
    </style>

    <style name="SpinnerStyle.Light">
        <item name="android:popupBackground">@color/panelBackground</item>
    </style>

    <style name="SpinnerStyle.Dark">
        <item name="android:popupBackground">@color/panelBackgroundDark</item>
    </style>

    <!-- TextView -->
    <style name="TextViewStyle" parent="Widget.AppCompat.TextView"/>
    <style name="TextViewStyle.Light">
        <item name="android:textColor">@color/textColor</item>
        <item name="textColor">@color/textColor</item>
    </style>
    <style name="TextViewStyle.Dark">
        <item name="android:textColor">@color/textColorDark</item>
        <item name="textColor">@color/textColorDark</item>
    </style>

    <!-- EditText -->
    <style name="EditTextStyle" parent="Widget.AppCompat.EditText"/>
    <style name="EditTextStyle.Light">
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textColorHint">@color/textColor</item>
    </style>
    <style name="EditTextStyle.Dark">
        <item name="android:textColor">@color/textColorDark</item>
        <item name="android:textColorHint">@color/textColorHintDark</item>
    </style>

    <!-- Button -->
    <style name="ButtonStyle" parent="Widget.AppCompat.Button">
        <item name="backgroundTint">?android:attr/colorButtonNormal</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <!-- FAB -->
    <style name="FloatingActionButtonStyle" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="tint">@color/colorPrimaryText</item>
        <item name="borderWidth">0dp</item>
        <item name="android:background">@null</item>
    </style>

    <!-- SearchView -->
    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="queryBackground">@android:color/transparent</item>
        <item name="submitBackground">@android:color/transparent</item>
        <item name="closeIcon">@drawable/ic_baseline_close</item>
        <item name="searchIcon">@drawable/ic_baseline_search</item>
        <item name="searchHintIcon">@drawable/ic_baseline_search</item>
        <item name="commitIcon">@null</item>
        <item name="goIcon">@null</item>
        <item name="textColor">@color/colorPrimaryText</item>
    </style>

    <style name="SearchViewStyle.Toolbar">
        <item name="textColor">@color/colorPrimaryText</item>
        <item name="autoCompleteTextViewStyle">@style/AutoCompleteTextView.Toolbar</item>
    </style>

    <!-- SearchView上の文字入力部分 -->
    <style name="AutoCompleteTextView.Toolbar" parent="Widget.AppCompat.AutoCompleteTextView">
        <!-- 入力位置のカーソル -->
<!--        <item name="android:textCursorDrawable">@drawable/searchview_cursor</item>-->
        <item name="android:textCursorDrawable">@null</item>
    </style>

    <!-- CheckBox -->
    <style name="CheckBoxStyle" parent="Widget.AppCompat.CompoundButton.CheckBox"/>
    <style name="CheckBoxStyle.Light">
        <item name="colorControlNormal">@color/textColor</item>
        <item name="android:textColor">@color/textColor</item>
    </style>
    <style name="CheckBoxStyle.Dark">
        <item name="colorControlNormal">@color/textColorDark</item>
        <item name="android:textColor">@color/textColorDark</item>
    </style>

    <!-- RecyclerView -->
    <style name="RecyclerView">
        <item name="android:scrollbarThumbVertical">@drawable/scrollbar_thumb</item>
        <item name="android:scrollbarSize">4dp</item>
        <item name="android:scrollbars">vertical</item>
    </style>

    <style name="RecyclerView.Linear">
        <item name="layoutManager">androidx.recyclerview.widget.LinearLayoutManager</item>
    </style>

    <!-- MarqueeTextView -->
    <style name="MarqueeTextView" parent="Widget.AppCompat.TextView">
        <item name="android:ellipsize">marquee</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="android:singleLine">true</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:duplicateParentState">true</item>
        <item name="android:scrollHorizontally">true</item>
    </style>

    <!-- スタースパン色 -->
    <style name="StarSpan" parent="@android:style/TextAppearance">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="StarSpan.Yellow">
        <item name="android:textColor">@color/starYellow</item>
    </style>
    <style name="StarSpan.Red">
        <item name="android:textColor">@color/starRed</item>
    </style>
    <style name="StarSpan.Green">
        <item name="android:textColor">@color/starGreen</item>
    </style>
    <style name="StarSpan.Blue">
        <item name="android:textColor">@color/starBlue</item>
    </style>
    <style name="StarSpan.Purple">
        <item name="android:textColor">@color/starPurple</item>
    </style>

    <!-- ボトムシート(View)の角を丸める -->
    <style name="ShapeAppearanceOverlay.RoundedCorner">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">12dp</item>
        <item name="cornerSizeTopRight">12dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

    <!-- BottomSheetDialogのテーマ -->
    <style name="BottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>
    <style name="BottomSheetDialogTheme.Light">
        <item name="android:textViewStyle">@style/TextViewStyle.Light</item>
        <item name="editTextStyle">@style/EditTextStyle.Light</item>
        <item name="bottomSheetStyle">@style/BottomSheet.Light</item>
    </style>
    <style name="BottomSheetDialogTheme.Dark">
        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="bottomSheetStyle">@style/BottomSheet.Dark</item>
    </style>
    <style name="BottomSheetDialogTheme.ExDark">
        <item name="android:textViewStyle">@style/TextViewStyle.Dark</item>
        <item name="editTextStyle">@style/EditTextStyle.Dark</item>
        <item name="bottomSheetStyle">@style/BottomSheet.ExDark</item>
    </style>

    <style name="BottomSheet" parent="Widget.Design.BottomSheet.Modal"/>
    <style name="BottomSheet.Light">
        <item name="android:background">@drawable/shape_bottom_sheet_rounded_corner</item>
    </style>
    <style name="BottomSheet.Dark">
        <item name="android:background">@drawable/shape_bottom_sheet_rounded_corner_dark</item>
    </style>
    <style name="BottomSheet.ExDark">
        <item name="android:background">@drawable/shape_bottom_sheet_rounded_corner_ex_dark</item>
    </style>
</resources>
